{
  "expo": {
    "name": "Paw Match",
    "slug": "pawmatch",
    "newArchEnabled": true,
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "pawmatch",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "xyz.pawmatch.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "xyz.pawmatch.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "server"
    },
    "extra": {
      "eas": {
        "projectId": "377a8753-286e-4d85-b414-311f117e53db"
      }
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": []
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/logo-splash.png",
          "dark": {
            "image": "./assets/logo-splash-white.png",
            "backgroundColor": "#1F1F1F"
          },
          "imageWidth": 200
        }
      ],
      "expo-router"
    ]
  }
}

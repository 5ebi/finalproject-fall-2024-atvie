{
  "expo": {
    "name": "finalproject-fall-2024-atvie",
    "slug": "finalproject-fall-2024-atvie",
    "newArchEnabled": true,
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "finalproject-fall-2024-atvie",

    "ios": {
      "supportsTablet": true
    },

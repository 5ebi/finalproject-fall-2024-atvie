        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "377a8753-286e-4d85-b414-311f117e53db"
      }
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "/node_modules/@expo-google-fonts/montserrat/Montserrat_900Black.ttf",
            "node_modules/@expo-google-fonts/inter/Inter_900Black.ttf",
            "/node_modules/@expo-google-fonts/modak/Modak_400Regular.ttf"
          ]
        }
      ]
